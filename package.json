  "scripts": {
    "dev": "concurrently \"next dev\" \"yarn codegen:watch\"",
    "build": "next build",
    "build:analyze": "ANALYZE=true yarn build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "test": "jest --forceExit --passWithNoTests",
    "test:ci": "yarn test",
    "test:watch": "jest --watch",
    "lint:fix": "yarn lint --fix; yarn format",
    "lint": "eslint --ignore-path .eslintignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .eslintignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "release": "release-it",
    "commit": "commit-it",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:watch": "yarn codegen --watch",
    "codegen:zeus": "zeus src/graphql/schema.graphql src/generated --typedDocumentNode"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .eslintignore --fix",
      "prettier --ignore-path .eslintignore --write"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "ðŸ”– | v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    }
  },
